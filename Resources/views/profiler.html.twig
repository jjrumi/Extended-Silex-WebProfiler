{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block toolbar %}
    {# the web debug toolbar content #}
    {% set icon %}
        <img alt="Propel" src="data:image/png;base64," />
        <span class="sf-toolbar-status">{{ collector.getBranch() }}</span> at <span>{{ collector.getRevision() }}</span>
    {% endset %}
    {% set text %}
        <div class="sf-toolbar-info-piece">
            <b>Branch</b>
            <span>{{ collector.getBranch() }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Revision</b>
            <span>{{ collector.getRevision() }}</span>
        </div>
    {% endset %}
    {% include '@WebProfiler/Profiler/toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}